@inherits FluentComponentBase
<fluent-dialog @ref=Element
               class="@Class"
               style="@Style"
               modal="@Modal"
               hidden="@(Hidden)"
               no-focus-trap=@NoFocusTrap
               aria-describedby=@AriaDescribedby
               aria-labelledby=@AriaLabelledby
               aria-label=@AriaLabel
               @attributes="AdditionalAttributes">
    @ChildContent
</fluent-dialog>
