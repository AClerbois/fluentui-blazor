@inherits FluentComponentBase
<fluent-radio @ref=Element
              class="@Class"
              style="@Style"
              readonly="@Readonly"
              id=@Id
              value="@Value"
              current-value=@CurrentValue
              disabled="@Disabled"
              name=@Name
              required="@Required"
              current-checked="@Checked"
              @attributes="AdditionalAttributes">
    @ChildContent
</fluent-radio>

