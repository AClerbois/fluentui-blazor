@namespace Microsoft.Fast.Components.FluentUI
@inherits FluentComponentBase
<fluent-tree-item @ref=Element
                  class="@Class"
                  style="@Style"
                  tree-item-id="@TreeItemId"
                  expanded="@Expanded"
                  selected="@Selected"
                  disabled="@Disabled"
                  @attributes="AdditionalAttributes">
    @Text
    @ChildContent
</fluent-tree-item>
