		<DefineStaticWebAssets Condition="'@(FlatAssetsCandidates-&gt;Count())' != '0'" CandidateAssets="@(FlatAssetsCandidates)" SourceId="$(PackageId)" SourceType="Discovered" AssetKind="All" ContentRoot="Assets/emojis" BasePath="$(StaticWebAssetBasePath)">
