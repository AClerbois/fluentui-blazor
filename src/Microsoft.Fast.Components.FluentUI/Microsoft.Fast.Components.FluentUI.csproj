		</DefineStaticWebAssets>

		<DefineStaticWebAssets Condition="'@(Filled10AssetsCandidates-&gt;Count())' != '0'" CandidateAssets="@(Filled10AssetsCandidates)" SourceId="$(PackageId)" SourceType="Discovered" AssetKind="All" ContentRoot="Assets/icons" BasePath="$(StaticWebAssetBasePath)">
